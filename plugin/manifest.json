{
  "name": "MyPocket",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Save pages on my pocket.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "scripts":["pocket.js"]
  }
}
